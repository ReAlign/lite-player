{
  "name": "lite-player",
  "version": "0.0.2",
  "description": "LitePlayer",
  "files": [
    "esm",
    "cjs",
    "es2017",
    "dist",
    "build"
  ],
  "module": "esm/index.js",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "es2017": "./es2017/index.js"
    },
    "./*": "./*"
  },
  "sideEffects": [
    "dist/*",
    "*.scss",
    "*.less",
    "*.css"
  ],
  "scripts": {
    "start": "ice-pkg start",
    "build": "ice-pkg build",
    "prepublishOnly": "npm run build",
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./src",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "stylelint \"./src/*.{css,scss,less}\"",
    "lint": "npm run eslint && npm run stylelint",
    "test": "node test/test.js"
  },
  "keywords": [
    "player",
    "lite",
    "lite player",
    "tiny",
    "tiny player"
  ],
  "devDependencies": {
    "@ice/pkg": "^1.0.0",
    "@ice/pkg-plugin-docusaurus": "^1.0.0",
    "@iceworks/spec": "^1.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "eslint": "^7.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "style-unit": "^3.0.4",
    "stylelint": "^13.7.2"
  },
  "peerDependencies": {
    "hls.js": "^1.2.9",
    "lodash.throttle": "^4.1.1",
    "react": "^17 || ^18",
    "react-input-range": "^1.3.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "dependencies": {
    "@swc/helpers": "^0.4.14"
  }
}
